        <name>osdRequest</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue === '') {
        return;
    }

    var ga = new GlideAjax('GetShipmentDetails'); // Script Include name
    ga.addParam('sysparm_name', 'getShipmentInfo'); // Function inside Script Include
    ga.addParam('sysparm_shipment_id', newValue);   // Input value (shipment ID)

    ga.getXMLAnswer(function(response) {
        if (response) {
            try {
                var data = JSON.parse(response);
                if (data.customer && data.product) {
                    g_form.setValue('customer', data.customer);
                    g_form.setValue('product', data.product);
                } else {
                    showError();
                }
            } catch (e) {
                showError();
            }
        } else {
            showError();
        }

        function showError() {
            g_form.showFieldMsg('shipment', 'Shipment not found or error retrieving data', 'error');
            g_form.setValue('customer', '');
            g_form.setValue('product', '');
        }
    });
}
]]></script>
